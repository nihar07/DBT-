import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;


public class dbt {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		dbt mydbt = new dbt();
		String temp = new String();
		String firstkeyword = new String();
		String input = mydbt.entercommand();
		
		 StringTokenizer st;
		
		while(input.toUpperCase().contains("EXIT")==false ){
			
			
			st = new StringTokenizer(input);
			firstkeyword = (String) st.nextElement();
			
			temp = mydbt.checktypestatement(firstkeyword);
			
			System.out.println(temp);
			input = mydbt.entercommand();
			
			
			
		}

	}

	public String entercommand() {
		System.out.print("DbtSQL>");
		String s = null;
 
	    try {
			BufferedReader bufferRead = new BufferedReader(
					new InputStreamReader(System.in));
			s = bufferRead.readLine();
			System.out.println(s);
		} catch (Exception e) {
			System.out.print("Please enter something good");
			// TODO: handle exception
		}
		return s;
		
	}
	
	
	//Checking statement type
	public String checktypestatement(String cs) {
		String stype = new String(); //checking statement type klike CREATE , INSERT or LOAD or SELECT
		
		
		if(cs.toUpperCase().contains("CREATE")  ){
			stype = "CREATE COMMAND";
		} else if(cs.toUpperCase().contains("INSERT")  ){
			stype = "INSERT COMMAND";
		} else if(cs.toUpperCase().contains("SELECT")  ){
			stype = "SELECT COMMAND";
		} else if(cs.toUpperCase().contains("LOAD")  ){
			stype = "LOAD COMMAND";
		}else if(cs.toUpperCase().contains("SAVE")  ){
			stype = "SAVE COMMAND";
		} else {
			stype = "Please enter something good";
		}
		
		return stype;
	}
	
	
	
	
}
